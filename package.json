{
  "name": "stock-eye",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "lint:css": "stylelint ./src/**/*.jsx",
    "//lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:eslint": "eslint --ext .ts,.tsx src",
    "lint": "run-p lint:*",
    "test": "jest",
    "build": "webpack --config webpack-web.config.js --mode production",
    "start": "webpack-dev-server --config webpack-web.config.js --mode development",
    "dev:api:webpack": "webpack --config webpack-app.config.js --mode development --watch",
    "dev:api:sam": "sam local start-api --debug",
    "dev:api": "run-p dev:api:*",
    "deploy:api": "run-s deploy:api:clean-build deploy:api:build deploy:api:upload-package deploy:api:deploy-stack",
    "//s1": "Step 1: Use ./aws-sam as output folder",
    "deploy:api:clean-build": "rm -rf ./.aws-sam",
    "//s2": "Step 2: Bundle source code to .aws-sam/build folder",
    "deploy:api:build": "webpack --config webpack-app.config.js --mode production",
    "//s3": "Step 3: Build SAM template file .aws-sam/template.yaml and upload package to s3://stockeye/packages",
    "deploy:api:upload-package": "sam package --output-template-file ./.aws-sam/template.yaml --s3-bucket stockeye --s3-prefix packages",
    "//s4": "Step 4: Deploy stack",
    "deploy:api:deploy-stack": "sam deploy --template-file ./.aws-sam/template.yaml --stack-name stockeye --capabilities CAPABILITY_IAM"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.703.0",
    "iconv-lite": "^0.6.0",
    "lodash-es": "^4.17.15",
    "prop-types": "^15.7.2",
    "re-reselect": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/plugin-proposal-do-expressions": "^7.10.1",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.1",
    "@babel/plugin-proposal-function-bind": "^7.10.1",
    "@babel/plugin-proposal-function-sent": "^7.10.1",
    "@babel/plugin-proposal-json-strings": "^7.10.1",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.10.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/plugin-proposal-numeric-separator": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.10.1",
    "@babel/plugin-proposal-throw-expressions": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.1",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/aws-lambda": "^8.10.56",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
