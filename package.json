{
  "name": "stock-eye",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "lint:css": "stylelint ./src/**/*.jsx",
    "lint:eslint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx src",
    "test": "jest",
    "build": "webpack --config webpack-web.config.js --mode production",
    "start": "webpack-dev-server --config webpack-web.config.js --mode development",
    "dev:api:webpack": "webpack --config webpack-app.config.js --mode development --watch",
    "dev:api:sam": "sam local start-api",
    "dev:api": "run-p dev:api:*",
    "deploy:api": "run-s deploy:api:clean-build deploy:api:build deploy:api:upload-package deploy:api:deploy-stack",
    "//s1": "Step 1: Use ./aws-sam as output folder",
    "deploy:api:clean-build": "rm -rf ./.aws-sam",
    "//s2": "Step 2: Bundle source code to .aws-sam/build folder",
    "deploy:api:build": "webpack --config webpack-app.config.js --mode production",
    "//s3": "Step 3: Build SAM template file .aws-sam/template.yaml and upload package to s3://stockeye/packages",
    "deploy:api:upload-package": "sam package --output-template-file ./.aws-sam/template.yaml --s3-bucket stockeye --s3-prefix packages",
    "//s4": "Step 4: Deploy stack",
    "deploy:api:deploy-stack": "sam deploy --template-file ./.aws-sam/template.yaml --stack-name stockeye --capabilities CAPABILITY_IAM"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.548.0",
    "iconv-lite": "^0.5.0",
    "lodash-es": "^4.17.15",
    "prop-types": "^15.7.2",
    "re-reselect": "^3.4.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.12.15",
    "reselect": "^4.0.0",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-do-expressions": "^7.6.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@hot-loader/react-dom": "^16.10.2",
    "@types/jest": "^24.0.18",
    "@types/lodash-es": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.3.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "js-yaml": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.10.2",
    "style-loader": "^0.23.1",
    "stylelint": "^11.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
