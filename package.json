{
  "name": "stock-eye",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "lint:css": "stylelint ./src/**/*.jsx",
    "lint:eslint": "eslint --ext .js --ext .jsx src",
    "test": "jest",
    "build": "webpack --config webpack-web.config.js --mode production",
    "start": "webpack-dev-server --config webpack-web.config.js --mode development",
    "dev:api:webpack": "webpack --config webpack-app.config.js --mode development --watch",
    "dev:api:sam": "sam local start-api",
    "dev:api": "run-p dev:api:*",
    "deploy:api": "run-s deploy:api:clean-build deploy:api:build deploy:api:upload-package deploy:api:deploy-stack",
    "//s1": "Step 1: Use ./aws-sam as output folder",
    "deploy:api:clean-build": "rm -rf ./.aws-sam",
    "//s2": "Step 2: Bundle source code to .aws-sam/build folder",
    "deploy:api:build": "webpack --config webpack-app.config.js --mode production",
    "//s3": "Step 3: Build SAM template file .aws-sam/template.yaml and upload package to s3://stockeye/packages",
    "deploy:api:upload-package": "sam package --output-template-file ./.aws-sam/template.yaml --s3-bucket stockeye --s3-prefix packages",
    "//s4": "Step 4: Deploy stack",
    "deploy:api:deploy-stack": "sam deploy --template-file ./.aws-sam/template.yaml --stack-name stockeye --capabilities CAPABILITY_IAM"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.589.0",
    "iconv-lite": "^0.5.0",
    "lodash-es": "^4.17.15",
    "prop-types": "^15.7.2",
    "re-reselect": "^3.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "reselect": "^4.0.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-do-expressions": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/plugin-proposal-function-sent": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-proposal-pipeline-operator": "^7.7.4",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@hot-loader/react-dom": "^16.11.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.3.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "flow-bin": "^0.113.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
