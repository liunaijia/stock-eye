    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "js-yaml": "^3.14.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^1.2.1",
