{
    "manifest_version": 2,
    "name": "StockEye",
    "description": "",
    "version": "0.1",
    "background": {
        "scripts": [
            "vendor.js",
            "background.js"
        ]
    },
    "permissions": [
        "http://hq.sinajs.cn/*",
        "http://q.stock.sohu.com/*",
        "https://etrade.newone.com.cn/*",
        "notifications"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}