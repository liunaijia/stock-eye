{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": "last 2 Chrome versions"
        },
        "modules": false
      }
    ]
  ],
  "plugins": ["transform-object-rest-spread"],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}